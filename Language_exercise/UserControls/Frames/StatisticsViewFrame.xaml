<UserControl x:Class="Language_exercise.UserControls.Frames.StatisticsViewFrame"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Language_exercise.UserControls.Frames"
             xmlns:view="clr-namespace:Language_exercise.UserControls"
             xmlns:viewModel="clr-namespace:Language_exercise.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="1080" d:DesignWidth="1620">
    <Grid>
        <ContentControl Content="{Binding Path=CurrentViewModel}">
            <ContentControl.Resources>
                <DataTemplate DataType="{x:Type viewModel:StatisticsViewModel}">
                    <view:LE_Statistics/>
                </DataTemplate>
            </ContentControl.Resources>
        </ContentControl>
            
    </Grid>
</UserControl>
